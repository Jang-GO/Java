import java.util.*;

class Main{
    public ArrayList<Integer> solution(int N,int[] arr){
        ArrayList<Integer> al = new ArrayList<>();
        int[] tmp=arr.clone();
        Arrays.sort(tmp);
        for(int i=0;i<N;i++){
            if(arr[i]!=tmp[i]) al.add(i+1);
        }
        return al;
    }
    public static void main(String[] args){
        Main T = new Main();
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        int[] arr = new int[N];
        for(int i=0;i<N;i++){
            arr[i] = sc.nextInt();
        }
        for(int x: T.solution(N, arr)){
            System.out.print(x+" ");
        }
    }
}
