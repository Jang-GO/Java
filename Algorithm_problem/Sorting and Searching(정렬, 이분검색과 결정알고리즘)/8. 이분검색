import java.util.*;
class Main{
    public int solution(int N,int M, int[] arr){
        int answer=0;
        int lt=0; int rt=N-1;
        Arrays.sort(arr);
        while(lt<=rt){
            int mid=(lt+rt)/2;
            if(arr[mid]==M){
                answer=mid+1;
                break;
            }
            if(arr[mid]>M) rt = mid-1;
            if(arr[mid]<M) lt = mid+1;
        }
        return answer;
    }
    public static void main(String[] args){
    Main T = new Main();
    Scanner sc = new Scanner(System.in);
    int N = sc.nextInt();
    int M = sc.nextInt();
    int[] arr = new int[N];
    for(int i=0;i<N;i++){
        arr[i] = sc.nextInt();
    }
    System.out.println(T.solution(N,M,arr));
    }
}
